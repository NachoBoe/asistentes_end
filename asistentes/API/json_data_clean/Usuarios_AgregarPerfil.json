{
    "id": 524,
    "nombre": "AgregarPerfil",
    "descripcion": "Método para agregar un perfil a un usuario en el sistema.",
    "entrada": {
        "usuario": {
            "type": "string",
            "description": "Identificador del usuario."
        },
        "sdtPerfil": {
            "type": "object",
            "description": "Datos del perfil.",
            "properties": {
                "Perfil": {
                    "type": "string",
                    "description": "Identificación de perfil Bantotal."
                },
                "FechaAlta": {
                    "type": "string",
                    "description": "Fecha a partir de la cual el usuario se encuentra habilitado para operar con el perfil."
                },
                "FechaVencimiento": {
                    "type": "string",
                    "description": "Fecha de vencimiento del perfil para el usuario."
                }
            },
            "required": [
                "Perfil",
                "FechaAlta",
                "FechaVencimiento"
            ]
        }
