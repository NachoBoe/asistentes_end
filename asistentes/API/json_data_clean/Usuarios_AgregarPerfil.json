                },
                "email": {
                    "type": "string",
                    "description": "Email del usuario."
                },
                "telefono": {
                    "type": "string",
                    "description": "Teléfono del usuario."
                },
                "sucursal": {
                    "type": "string",
                    "description": "Sucursal donde se encuentra el usuario."
                },
                "programaInicial": {
                    "type": "string",
                    "description": "Nombre de programa incial que se invoca al acceder a Bantotal."
                },
                "espaciosDeTrabajo": {
                    "type": "number",
                    "description": "Cantidad máxima de espacios de trabajos que puede tener abierta el usuario."
                },
                "cantidadSesionesAbiertas": {
